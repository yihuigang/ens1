{
    "name": "logwiresnip",
    "displayName": "logwire代码片段",
    "description": "logwire代码片段",
    "version": "0.3.1",
    "engines": {
        "vscode": "^1.61.0"
    },
    "publisher":"yhg",
    "categories": [
        "Snippets"
    ],
    "icon": "images/icon2.png",
    "contributes": {
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/js_snippets.code-snippets"
            },
            {
                "language": "xml",
                "path": "./snippets/xml_snippets.code-snippets"
            },
            {
                "language": "xml",
                "path": "./snippets/model.code-snippets"
            },
            {
                "language": "xml",
                "path": "./snippets/layout_list.code-snippets"
            }
        ],

        "commands": [
            {
                "command": "extension.openUrl",
                "title": "运匠",
                "path": "./src/extension.js"
            }
        ],
        "menus": {
            "editor/context": [
                {
                    "when": "editorFocus",
                    "command": "extension.openUrl",
                    "group": "navigation"
                }
            ]
        }
    }
}
